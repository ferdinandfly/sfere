/**
 * @ORM\Entity
 * @ORM\Table(name="country_trans")
 */
class CountryTranslation {
    //use ORMBehaviors\Translatable\Translation;

    /**
     * @ORM\Column(type="string", length=255, nullable=true)
     */
    protected $name;
} 