<div class="submenu">
    <ul class="list_submenu">
        {% for category in categories %}
        <li>
            <a href="{{ path('sfere_category_detail',{'id':category.id}) }}">
                <span class="icon_products" id="icon_analogica">{{ category.name }}</span></a>
            <ul>
            {% for subcategory in category.subCategories %}
                <li><a href="{{ path('sfere_sub_category_product',{'id': subcategory.id}) }}">{{ subcategory.name }}</a></li>
            {% endfor %}
            </ul>
        </li>
        {% endfor %}
    </ul>
</div>
